<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.attendancemanager">

    <application
        android:allowBackup="true"
        android:icon="@drawable/macbook"
        android:label="@string/app_name"
        android:roundIcon="@drawable/macbook"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StudentActivity" >

        </activity>
        <activity android:name=".UserListActivity" />
        <activity
            android:name=".AddStudent"
            android:theme="@style/Theme.MaterialComponents.Light.DarkActionBar.Bridge" />
        <activity
            android:name=".Attendance"
            android:theme="@style/Theme.MaterialComponents.Light.DarkActionBar.Bridge" />
        <activity android:name=".FacultyActivity1" />
        <activity android:name=".AdminActivity1" />
        <activity
            android:name=".Login2"
            android:theme="@style/Theme.MaterialComponents.Light.DarkActionBar.Bridge" />
        <activity android:name=".Login1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SplashScreen"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>